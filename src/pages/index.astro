                    section_five_button
                    section_five_video
                    section_six_title
                    section_six_content
                    section_six_button
                    section_six_video
                    section_seven_title
                    section_seven_content
                    section_seven_button
                    section_eight_title
                    section_eight_content
                    section_eight_button
                    section_metaverse_title
                      metavarse_development_lists{
                        data{
                          attributes {
                            title
                            content
                            image
                          }
                        }
                    }
                    section_metaverse_developmet
                      metavarse_development_service_lists( pagination: { limit: 100 }){
                        data{
                            attributes{
                                image
                          }
                        }
                      }
                      section_metaverse_domain_title
                      metaverse_domain_solutions{
                        data{
                          attributes{
                            text
                            image
                            content
                          }
                        }
                      }
                      section_our_technologies
                      metaverse_our_technologies{
                        data{
                          attributes{
                            image
                          }
                        }
                      }
                      section_why_metaprolane
                      section_metaprolane_description
                      metaverse_why_metaprolanes{
                        data{
                          attributes{
                          image
                            text
                            content
                          }
                        }
                      }
                      metaverse_lists{
                        data{
                          attributes{
                            title
                            content
                          }
                        }
                      }
                      metaverse_faq_title
                      metaverse_faq_lists {
                        data{
                            attributes{
                                title
                                content
                            }
                        }
                      }
                      
                }
            }
        }
    }
        `,
    }),
});
const data = await response.json();

const activeIndex = 1;
