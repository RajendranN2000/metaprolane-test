                    });
                });
                $(document).ready(() => {
                    $(".readmore-description").click(function () {
                        var $cardTextContent = $(this).prev(
                            ".card1-text-content",
                        );
                        $cardTextContent.toggleClass("expanded");
                        $(this).text(
                            $cardTextContent.hasClass("expanded")
                                ? "Read less"
                                : "Read more",
                        );
                    });
                });
                $(document).ready(() => {
                    $(".wallet-content").each(function () {
                        var $content = $(this);
                        if (
                            $content[0]?.scrollHeight > $content.innerHeight()
                        ) {
                            $content.next(".virtual-readmore").show();
                        } else {
                            $content.next(".virtual-readmore").hide();
                        }
                    });
                    $(".virtual-readmore").click(function () {
                        var $cardTextContent = $(this).prev(".wallet-content");
                        $cardTextContent.toggleClass("virtual-expanded");
                        $(this).text(
                            $cardTextContent.hasClass("virtual-expanded")
                                ? "Read less"
                                : "Read more",
                        );
                    });
                });
                $(document).ready(function () {
                    $(".talk-btn").click(function () {
                        $("html, body").animate(
                            {
                                scrollTop: $("#footer").offset().top,
                            },
                            300,
                        ); // Adjust the duration as per your requirement
                    });
                });
                // $("#content").hide().show("slide", { direction: "left" }, 1500);
