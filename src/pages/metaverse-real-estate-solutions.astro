const response = await fetch(`${import.meta.env.PUBLIC_STRAPI_URL}`, {
    method: "POST",
    headers: { "Content-Type": "application/json" },
    body: JSON.stringify({
        query: `
        query{
            metaverseRealEstateSolution{
              data {
                attributes {
                 real_estate_dev_title
                  real_estate_dev_content
