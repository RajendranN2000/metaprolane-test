                    data{
                      attributes{
                        title
                        content
                      }
                    }
                  }
                  real_estate_minim_veniam_title
                  real_estate_minim_veniam_lists {
                    data{
                      attributes{
                        title
                        content
                        image
                      }
                    }
                  }
                  real_estate_benefits_title
                  real_estate_benefits_lists{
                    data{
                      attributes{
                        title
                        content
                        image
                      }
                    }
                  }
                  real_estate_platform_dev_title
                  real_estate_platform_dev_content
                  real_estate_platform_dev_lists{
                    data{
                      attributes{
                        title
                        content
                        image
                      }
                    }
                  }
                  real_estate_platform_mechanics
                  real_estate_platform_mechanics_lists{
                    data{
                      attributes{
                        title
                        content
                      }
                    }
                  }
                  real_estate_tech_stack_title
                  real_estate_choose_metaprolane_title
                  real_estate_choose_metaprolane_lists{
                    data{
                      attributes{
                        title
                        content
                      }
                    }
                  }
                
             
                  
                }
              }
                
            } 
        }
        `,
    }),
});

const { data } = await response.json();
