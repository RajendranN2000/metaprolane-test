<Layout title="metaverse-virtual-office" description="" keywords="">
    <html lang="en">
        <div class="home-container industries-page meta-product">
            <link rel="stylesheet" href="/styles/css/retailShowroom.css" />
            <script
                is:inline
                src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"
            ></script>
            <link
                href="/vendor/bootstrap/css/bootstrap.min.css"
                rel="stylesheet"
            />
            <link rel="stylesheet" href="/styles/css/style.css" />

            <script is:inline>
                $(document).ready(function () {
                    $(".question1").click(function () {
                        console.log("clicked questions");
                        var answer = $(this).next(".answer");
                        $(".answer").not(answer).slideUp();
                        answer.slideToggle();
                    });
                });
                $(document).ready(() => {
                    $(".readmore-description").click(function () {
                        var $cardTextContent = $(this).prev(
                            ".card1-text-content",
                        );
                        $cardTextContent.toggleClass("expanded");
                        $(this).text(
                            $cardTextContent.hasClass("expanded")
                                ? "Read less"
                                : "Read more",
                        );
                    });
                });
                $(document).ready(() => {
                    $(".wallet-content").each(function () {
                        var $content = $(this);
                        if (
                            $content[0]?.scrollHeight > $content.innerHeight()
                        ) {
                            $content.next(".virtual-readmore").show();
                        } else {
                            $content.next(".virtual-readmore").hide();
                        }
                    });
                    $(".virtual-readmore").click(function () {
                        var $cardTextContent = $(this).prev(".wallet-content");
                        $cardTextContent.toggleClass("virtual-expanded");
                        $(this).text(
                            $cardTextContent.hasClass("virtual-expanded")
                                ? "Read less"
                                : "Read more",
                        );
                    });
                });
                $(document).ready(function () {
                    $(".talk-btn").click(function () {
                        $("html, body").animate(
                            {
                                scrollTop: $("#footer").offset().top,
                            },
                            300,
                        ); // Adjust the duration as per your requirement
                    });
                });
                // $("#content").hide().show("slide", { direction: "left" }, 1500);
            </script>

